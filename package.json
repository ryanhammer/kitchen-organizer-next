{
  "name": "kitchen-organizer-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:db:setup": "prisma generate && prisma migrate dev && prisma db seed",
    "dev:db:apply-new-migrations": "prisma migrate dev",
    "build": "next build",
    "start": "next start",
    "format": "npx @biomejs/biome format --write ./src",
    "lint": "next lint",
    "prod:db:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "script:populate-ingredient-and-branded-ingredient-tables": "ts-node --transpile-only -r dotenv/config src/scripts/one-time-scripts/populate-ingredient-and-branded-ingredient-tables.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.5.1",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@faker-js/faker": "^8.3.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "schema": "./src/db/schema.prisma",
    "seed": "ts-node -r tsconfig-paths/register ./src/db/seed.ts"
  }
}
